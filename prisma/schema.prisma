generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_abri")
}

model Registration {
  id              String    @id @default(cuid())
  fullName        String
  nationalId      String    @unique
  birthDate       DateTime?
  city            String
  level           String
  mobileNumber    String
  emergencyNumber String
  profileImage    String
  receipts        String[]
  createdAt       DateTime  @default(now())
  updatedAt       DateTime  @updatedAt
}